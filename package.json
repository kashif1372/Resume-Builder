{
  "name": "resumebuilder",
  "version": "1.0.0",
  "description": "Resume Builder ",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.0",
    "npm": "9.5.0"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server \" \"npm run client\" ",
    "start": "node server.js"
  },
  "keywords": [
    "resume",
    "builder"
  ],
  "author": "Kashif",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.2.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "mongoose": "^6.5.0",
    "node": "^20.7.0"
  },
  "proxy": {
    "/auth/google": {
      "target": "localhost:5000"
    }
  }
}
